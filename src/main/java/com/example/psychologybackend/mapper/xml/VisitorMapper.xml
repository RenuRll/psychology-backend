<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.psychologybackend.mapper.VisitorMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.psychologybackend.entity.Visitor">
        <id column="vid" property="vid" />
        <result column="open_id" property="openId" />
        <result column="name" property="name" />
        <result column="phone" property="phone" />
        <result column="emg_ct_name" property="emgCtName" />
        <result column="emg_ct_phone" property="emgCtPhone" />
        <result column="avatar_url" property="avatarUrl" />
        <result column="status" property="status" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        vid, open_id, name, phone, emg_ct_name, emg_ct_phone, avatar_url, status
    </sql>

</mapper>
